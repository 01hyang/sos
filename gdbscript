# arm-none-eabi-gdb -x gdbscript

# Gives info about where symbols are!
#file kernel.elf
file post_mmu.elf

# May not be necessary?
set architecture arm

# Connect to qemu remotely on port 9000
target remote localhost:9000

# Gets us a nice GUI
tui enable
layout regs

# These guys skip over the initial instructions which will jump to the entry
# point. GDB gets (rightfully) confused about them.
break main
continue
